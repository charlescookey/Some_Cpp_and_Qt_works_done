class Solution {
public:
    double myPow(double x, int n) {
        if(x =1)return 1;
        double ans =x;
        double time = n>0?n-1: (n*-1)-1;
        
        for(int i =0 ; i < time ; i++){
            ans*=x;
        }
        cout<<ans<<" "<<n<<endl;
        
        if(n > 0)return ans;
        else if(n < 0)return 1/ans;
        else if(n ==0)return 1;
        
        return ans;

    }
};
