class Solution {
public:
    vector<vector<int>> generate(int numRows) {
        vector<vector<int>> ans{};
        if(numRows == 0)return ans;
        ans.push_back({1});
        if(numRows == 1)return ans;
        ans.push_back({1,1});

        for(int i =2; i < numRows ; i++ ){
            vector<int> temp{};
            for(int j =0 ; j<= i ; j++){
                if(j == 0 || j == i){
                    temp.push_back(1);
                    continue;
                }
                temp.push_back(ans[i-1][j-1] + ans[i-1][j]);                
            }
            ans.push_back(temp);
        }
        return ans;
        
    }
};
