class Solution {
public:
    void reverseString(vector<char>& s) {
        for(int i = 0 , j = s.size()-1 ; i < s.size()/2 ; i++ , j--){
            std::swap(s[i],s[j]);
        } 
        
    }
};



really intereting solution, not be me though
class Solution {
public:
    string reverseString(string s) {
        return { s.rbegin(), s.rend() };
    }
};
